    {
      "endpoint": "/v1",
      "method": "GET",
      "output_encoding": "json",
      "backend": [
        {
          "type": "internal",
          "url_pattern": "/v2",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "http://localhost:8080"
          ],
          "disable_host_sanitize": false,
          "group": "backend11"
        },
        {
          "url_pattern": "/httpRequest/test.php",
          "encoding": "json",
          "sd": "static",
          "method": "GET",
          "host": [
            "https://Json.Ge"
          ],
          "disable_host_sanitize": false,
